{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "./src",
      "paths": {
        "*": ["node_modules/@types/*"]
      }
    },
    "include": ["src"],
    "exclude": ["node_modules", "build"]
  }
  